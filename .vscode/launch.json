    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Chatbot",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/Chatbot/movies.py",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.envrc",
        },
        {
            "name": "Python Debugger: openapi2tools",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/openapi2tools/tool.py",
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/MockServer/openapi.json",
                "${workspaceFolder}/output/moviesApi.py"
            ]
        },
    ]
}